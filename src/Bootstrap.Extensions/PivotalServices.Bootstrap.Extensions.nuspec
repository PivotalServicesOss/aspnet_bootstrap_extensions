<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>PivotalServices.AspNet.Bootstrap.Extensions</id>
    <version>$Version$</version>
    <title>PivotalServices.AspNet.Bootstrap.Extensions</title>
    <authors>ajaganathan,Pivotal</authors>
    <owners>ajaganathan</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <description>ASP.NET framework bootstrap extensions package which helps in managing IoC using DI containers (Autofac, Unity and ServiceCollection) and creating dynamic http handlers on a fly</description>
    <copyright>$PivotalServicesBootstrapCopyright$</copyright>
    <repository type="git" url="https://github.com/alfusinigoj/pivotal_aspnet_bootstrap_extensions.git"/>
    <tags>ASP.NET;Handlers;IoC</tags>
    <dependencies>
      <group targetFramework="$TargetFrameworkGeneral$">
        <dependency id="Microsoft.AspNet.Mvc" version ="$MicrosoftAspNetVersion$"/>
        <dependency id="Microsoft.AspNet.WebApi.WebHost" version ="$MicrosoftAspNetVersion$"/>
        <dependency id="Microsoft.Extensions.Hosting" version ="$MicrosoftExtensionsVersion$"/>
        <dependency id="Microsoft.Extensions.DependencyInjection.Abstractions" version ="$MicrosoftExtensionsVersion$"/>
        <dependency id="Microsoft.Extensions.Logging.Console" version ="$MicrosoftExtensionsVersion$"/>
        <dependency id="Microsoft.Extensions.Logging.Debug" version ="$MicrosoftExtensionsVersion$"/>
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Web"  targetFramework="$TargetFrameworkGeneral$" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="readme.txt" target="" />
  </files>
</package>
